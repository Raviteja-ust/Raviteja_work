package com.ust.validation;

//Online Java Compiler
//Use this editor to write, compile and run your Java code online
import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;



class EmployeeServiceValidation {
 public static void main(String[] args) {

     List<String> list=Arrays.asList("asdbah","w21321321","ASDFG1234A");

     List<String> collect = list.stream().
     filter(a->(a.length()==10)
&& (a.substring(0, 5).matches("[A-Z]{5}"))
&& (a.substring(5, 9).matches("[0-9]{4}"))
&& (a.substring(9, 10).matches("[A-Z]{1}"))
             ).collect(Collectors.toList());
     System.out.println(collect);



 }



}