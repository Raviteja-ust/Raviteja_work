package com.ust.validation;

import java.util.List;
import java.util.stream.Collectors;

public class EmployeeServiceValidation {
	
	
	public List<EmpModel> getEmployeeSort() {
		
		
		List<EmpModel> model = new EmployeeDao().getModel();
		List<EmpModel> PanList = model.stream().filter(a->a.getPanNumber().length()==10).collect(Collectors.toList());
		System.out.println(PanList);
		return model;
		
	}
	

	public static void main(String[] args) {
		

	}

}
