package com.ust;

import java.util.HashMap;
import java.util.Map;

public class Test {

	
	public static void main(String[] args) {
		demo d = new demo();
		demo d1 = d;
		d.a+=1;
		d.b+=1;
		d.print();
		d1.print();
}

	
}
class demo{
	int a,b;
	demo(){
		a=10
		b=20;
	}
	
	public void print() {
		System.out.println("a="+a+" b="+b+"");
	}
}


